	<parameter name="AD_Org_ID" isForPrompting="false" class="java.math.BigDecimal">
		<defaultValueExpression ><![CDATA[new BigDecimal(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="FechaDesde" isForPrompting="false" class="java.util.Date"/>
	<parameter name="AD_CLIENT_ID" isForPrompting="false" class="java.lang.Integer"/>
	<parameter name="FechaHasta" isForPrompting="false" class="java.util.Date"/>
	<parameter name="C_BPartner_ID" isForPrompting="false" class="java.math.BigDecimal"/>
	<queryString><![CDATA[SELECT 
	org.value AS cod_org,
	org.name AS nom_org,
	bp.value || '-' || bp.name AS proveedor,
	r.movementdate,
	o.documentno AS num_oc,
	r.description AS descripcion,
	u.name AS usuario,
	p.value || '-' || p.name AS articulo,
	rl.qtyentered AS cant,
	determineproductpricestd(rl.m_product_id, rl.ad_org_id, 'N') * rl.qtyentered AS cost
FROM 
	m_inoutline rl
	INNER JOIN m_inout r ON rl.m_inout_id = r.m_inout_id
	LEFT JOIN c_order o ON r.c_order_id = o.c_order_id
	INNER JOIN ad_org org ON r.ad_org_id = org.ad_org_id
	INNER JOIN c_bpartner bp ON r.c_bpartner_id = bp.c_bpartner_id
	INNER JOIN m_product p ON rl.m_product_id = p.m_product_id
	INNER JOIN AD_WF_Process wf ON (r.m_inout_id = wf.record_id AND wf.ad_table_id = 319)
	INNER JOIN ad_user u ON wf.ad_user_id = u.ad_user_id
WHERE 
	r.movementtype = 'V-'
	AND (($P{AD_Org_ID} IS NULL) OR ($P{AD_Org_ID} = 0) OR (rl.ad_org_id = $P{AD_Org_ID}))
	AND (rl.ad_client_id = $P{AD_CLIENT_ID})
	AND (($P{FechaDesde}::date IS NULL) OR (r.movementdate::date >= $P{FechaDesde}::date)) 
	AND (($P{FechaHasta}::date IS NULL) OR (r.movementdate::date <= $P{FechaHasta}::date))
	AND (($P{C_BPartner_ID} IS NULL) OR (r.c_bpartner_id = $P{C_BPartner_ID}))
ORDER BY r.movementdate DESC;]]></queryString>

